Algoritmo "Lista 1 - EX 10"
// 
//  
//
Var
//
  a,b,c,d : real
  det: real


Inicio
//
    Escreval("Digite o valor de a")
    leia(a)
    Escreval("Digite o valor de b")
    leia(b)
    Escreval("Digite o valor de c")
    leia(c)
    Escreval("Digite o valor de d")
    leia(d)
    
    det <- (a * d) - ( b * c)
    
    escreval("O VALOR DO DETERMINANTE E = ",det:2:2)

Fimalgoritmo
